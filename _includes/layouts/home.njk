---
layout: layouts/base.njk
---

{{ content | safe }}

<div id="search"></div>

<nav>
    <ul>
        {% for cat in search.pages("type=category tags.length=0") | reverse %}
        <li>
            <h3>
                <a href="{{ cat.url | url }}">
                    {{ cat.title }}
                </a>
            </h3>
            {{ cat.content | safe }}
        </li>
        {% endfor %}
    </ul>
</nav>