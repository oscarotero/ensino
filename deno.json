{
  "imports": {
    "cms/": "https://cdn.jsdelivr.net/gh/lumeland/cms@2d83bd982a960db7a7b10fd05498dd1906dce057/",
    "std/": "https://deno.land/std@0.213.0/",
    "lume/": "https://deno.land/x/lume@v2.0.3/",
    "hono/": "https://deno.land/x/hono@v3.11.11/"
  },
  "tasks": {
    "cms": "deno run -A ./_cms.ts",
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
